@base <http://virtual-assembly.org/ontologies/pair-tutor#> .

<http://virtual-assembly.org/ontologies/pair-tutor#> rdf:type owl:Ontology ;
                                                owl:imports <http://virtual-assembly.org/ontologies/pair> .

#################################################################
#    Object Properties
#################################################################

###  http://virtual-assembly.org/ontologies/pair-tutor#hasRegistration
:currentLesson rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf <http://virtual-assembly.org/ontologies/pair#interSubjectProperty> ;
               rdfs:domain :Registration;
               rdfs:range :Lesson .


###  http://virtual-assembly.org/ontologies/pair-tutor#hasRegistration
:hasRegistration rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf <http://virtual-assembly.org/ontologies/pair#interSubjectProperty> ;
                 owl:inverseOf :registrationFor ;
                 rdfs:domain :Course;
                 rdfs:range :Registration .


###  http://virtual-assembly.org/ontologies/cdlt#registeredFor
:registeredFor rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf <http://virtual-assembly.org/ontologies/pair#interSubjectProperty> ;
               owl:inverseOf :registrant ;
               rdfs:domain <http://virtual-assembly.org/ontologies/pair#Person> ;
               rdfs:range :Registration .


###  http://virtual-assembly.org/ontologies/pair-tutor#registrant
:registrant rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf <http://virtual-assembly.org/ontologies/pair#interSubjectProperty> ;
            owl:inverseOf :registeredFor ;
            rdfs:domain :Registration ;
            rdfs:range <http://virtual-assembly.org/ontologies/pair#Person> .


###  http://virtual-assembly.org/ontologies/pair-tutor#registrationFor
:registrationFor rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf <http://virtual-assembly.org/ontologies/pair#interSubjectProperty> ;
                 owl:inverseOf :hasRegistration ;
                 rdfs:domain :Registration ;
                 rdfs:range :Course .


#################################################################
#    Data properties
#################################################################

:duration rdf:type owl:DatatypeProperty ;
      rdfs:domain :Lesson ;
      rdfs:label "Durée (jours)"@fr ;
      rdfs:range xsd:integer .


:lessonStarted rdf:type owl:DatatypeProperty ;
               rdfs:domain :Registration ;
               rdfs:label "Début de la leçon actuelle"@fr ;
               rdfs:range xsd:datetime .


:order rdf:type owl:DatatypeProperty ;
      rdfs:domain :Lesson ;
      rdfs:label "Ordre par rapport aux autres leçons"@fr ;
      rdfs:range xsd:integer .


:video rdf:type owl:DatatypeProperty ;
      rdfs:domain :Lesson ;
      rdfs:label "Lien vidéo"@fr .



#################################################################
#    Classes
#################################################################

###  http://virtual-assembly.org/ontologies/pair-tutor#Course
:Course rdf:type owl:Class ;
        rdfs:subClassOf <http://virtual-assembly.org/ontologies/pair#Event> ;
        rdfs:label "Parcours"@fr .


###  http://virtual-assembly.org/ontologies/pair-tutor#CourseStatus
:CourseStatus rdf:type owl:Class ;
              rdfs:subClassOf <http://virtual-assembly.org/ontologies/pair#EventStatus> .


###  http://virtual-assembly.org/ontologies/pair-tutor#Course
:DigitalCourse rdf:type owl:Class ;
        rdfs:subClassOf :Course ;
        rdfs:label "Parcours en ligne"@fr .


###  http://virtual-assembly.org/ontologies/pair-tutor#Registration
:Lesson rdf:type owl:Class ;
              rdfs:subClassOf <http://virtual-assembly.org/ontologies/pair#Subject> ;
              rdfs:label "Leçon"@fr .


###  http://virtual-assembly.org/ontologies/pair-tutor#Registration
:Registration rdf:type owl:Class ;
              rdfs:subClassOf <http://virtual-assembly.org/ontologies/pair#Subject> ;
              rdfs:label "Inscription"@fr .


###  http://virtual-assembly.org/ontologies/pair-tutor#RegistrationStatus
:RegistrationStatus rdf:type owl:Class ;
              rdfs:subClassOf <http://virtual-assembly.org/ontologies/pair#EventStatus> .


